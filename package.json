{
    "name": "@trops/dash-react",
    "version": "0.1.32",
    "description": "A Dashboard Generator Package.",
    "main": "dist/index.js",
    "publishConfig": {
        "@trops:registry": "https://npm.pkg.github.com",
        "access": "restricted"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "npm run prettify && npm run build:css && npm run prod",
        "build:css": "tailwindcss -i src/index.css -o src/tailwind.css -m",
        "release-workflow": "",
        "prod": "npm run prettify && npm run clean-dist && npm run clean-package && npm run roll && npm run copy && npm run pack-local-esm && npm run copy-package",
        "dry": "npm run prettify && yarn bump && rm -r ./dist/* && rm -r ./package/* && npm run roll && yarn copy && yarn pack-local-esm && yarn copy-package",
        "clean-dist": "rm -r ./dist/* || exit 0",
        "clean-package": "rm -r ./package/* || exit 0",
        "copy": "\\cp package.json dist/ && cp README.md dist/ && \\cp jsconfig.json dist/ && cp src/tailwind.css dist/tailwind.css && \\cp .npmrc dist/.npmrc",
        "copy-package": "\\cp ./package/$npm_package_name-$npm_package_version.tgz ./package/$npm_package_name.tgz || exit 0",
        "pack-local-esm": "cd ./dist && npm pack --pack-destination ../package",
        "link-package-1": "cd ./dist && npm link",
        "roll": "rollup -c",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "bump": "npm version --no-git-tag-version patch",
        "bump-tag": "npm version -m patch",
        "prettify": "yarn prettier --write . ",
        "test-jest": "NODE_OPTIONS=--experimental-vm-modules jest",
        "test": "exit 0"
    },
    "keywords": [
        "Dash"
    ],
    "author": "John P. Giatropoulos <john.giatropoulos@gmail.com>",
